{
  "name": "@aave/types",
  "version": "1.0.0-next.2",
  "description": "Core types for the Aave SDK",
  "repository": {
    "directory": "packages/types",
    "type": "git",
    "url": "git://github.com/aave/aave-v4-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "import": [
        "./dist/index.d.ts"
      ],
      "require": [
        "./dist/index.d.cts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@types/big.js": "^6.2.2",
    "big.js": "^7.0.1",
    "neverthrow": "^8.2.0",
    "type-fest": "^5.1.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
